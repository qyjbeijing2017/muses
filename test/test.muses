Shader "Test" {
    Properties {
        _DiffuseTex ("Diffuse Texture", 2D) = "white" {}
        _NormalTex ("Normal Texture", 2D) = "white" {}
        _SpecularTex ("Specular Texture", 2D) = "white" {}
        _Shininess ("Shininess", Range(0, 1)) = 0.5
    }
    
    SubShader {
        Pass {
            // renderstate

            // glsl
            GLSLPROGRAM

            struct VertexInput {
                vec3 position;
                vec3 normal;
                vec2 texcoord;
                float y;
            };

            void vert(VertexInput a2v) {
                if(a2v.y<3.){
                    gl_Position = vec4(a2v.position, 1.0);
                }else if(a2v.y>4.&&a2v.y<7.){
                    gl_Position = vec4(a2v.position, 1.0);
                }else{
                    gl_Position = vec4(a2v.position, 1.0);
                }
                gl_Position = vec4(a2v.position.xy, a2v.position.yz);
            }

            void frag() {
                gl_FragColor = vec4(0, 0, 0, 0);
            }

            ENDGLSL
        }
    }

    FallBack "Diffuse"
}